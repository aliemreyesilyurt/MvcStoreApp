@model IEnumerable<Order>

@if(Model.Count() > 0)
{
        @foreach (Order order in Model)
        {
            <div class="my-3">
                <partial name="~/Areas/Admin/Views/Order/Partials/_OrderDetails.cshtml" model="@order"/>

            </div>
        }
}
else
{
    <div class="alert alert-info">
        <p class="lead">There is no order yet</p>
    </div>
}